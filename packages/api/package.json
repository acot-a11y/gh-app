{
  "name": "api",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev -r dotenv/config -- src/index.ts",
    "start": "node -r dotenv/config lib/index.js",
    "test": "jest"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/.*/__mocks__"
    ],
    "preset": "ts-jest",
    "rootDir": "src",
    "setupFiles": [
      "<rootDir>/../jest.setup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*.test.ts?(x)"
    ]
  },
  "dependencies": {
    "@acot/gh-app-shared": "workspace:*",
    "@google-cloud/datastore": "6.6.2",
    "@google-cloud/firestore": "5.0.2",
    "@octokit/rest": "18.12.0",
    "@types/bunyan": "1.8.8",
    "@types/safe-json-stringify": "1.1.2",
    "bunyan": "1.8.15",
    "bunyan-format": "0.2.1",
    "dotenv": "10.0.0",
    "fastify": "3.29.0",
    "fastify-cors": "6.0.2",
    "fastify-plugin": "3.0.1",
    "got": "11.8.3",
    "jsonwebtoken": "8.5.1",
    "markdown-table-ts": "1.0.3",
    "pretty-ms": "7.0.1",
    "safe-json-stringify": "1.2.0",
    "typed-inject": "3.0.1"
  },
  "devDependencies": {
    "@acot/factory": "0.0.16",
    "@acot/gh-app-types": "workspace:*",
    "@acot/tsconfig": "0.1.0",
    "@acot/types": "0.0.16",
    "@types/bunyan-format": "0.2.4",
    "@types/jest": "27.4.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "16.18.91",
    "jest": "27.4.5",
    "nock": "13.2.4",
    "ts-jest": "27.1.4",
    "ts-node-dev": "1.1.8",
    "type-fest": "2.8.0",
    "typescript": "4.5.4"
  }
}
